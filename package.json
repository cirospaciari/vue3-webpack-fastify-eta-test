{
  "name": "fastify-hibrid",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "dev": "concurrently \"WEBPACK_WATCH=true npm run build:dev\" \"sleep 3 && nodemon -e js,css,scss,json,html,xml,png,svg,jpg,jpeg,webp,apng,avif,jfif,pjpeg,pjp,ico,cur,tif,tiff,txt,eta index.js --delay 1\"",
    "build:stg": "WEBPACK_MODE=production NODE_ENV=development webpack --config ./webpack.config.js",
    "build:dev": "WEBPACK_MODE=development NODE_ENV=development webpack --config ./webpack.config.js",
    "build": "WEBPACK_MODE=production NODE_ENV=production webpack --config ./webpack.config.js"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@svgr/webpack": "^5.5.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "eta": "^1.12.3",
    "fastify": "^3.24.0",
    "fastify-plugin": "^3.0.0",
    "fastify-static": "^4.5.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.4.5",
    "nodemon": "^2.0.15",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "source-map-loader": "^3.0.0",
    "url-loader": "^4.1.1",
    "vue": "^3.2.22",
    "vue-loader": "^16.8.3",
    "webpack": "^5.64.1",
    "webpack-assets-manifest": "^5.0.6",
    "webpack-cli": "^4.9.1",
    "webpack-virtual-modules": "^0.4.3"
  }
}
